#include<iostream>
using namespace std;

void rotate(int arr[],int n,int pos)
{
     int b[n];
     int j=0;
     for(int i=pos;i<n;i++)
     {
        b[j++]=arr[i];
     }
     for(int i=0;i<pos;i++)
     {
        b[j++]=arr[i];
     }
     for(int i=0;i<n;i++)
     arr[i]=b[i];
}
void display(int arr[],int n )
{
    for(int i=0;i<n;i++)
    cout<<arr[i]<<" ";
}
int main()
{
   int n;
   cin>>n;
   int arr[n];

   for(int i=0;i<n;i++)
   {
    cin>>arr[i];
   }
   int key;
   cout<<"Enter key value "<<endl;
   cin>>key;

    rotate(arr,n,key);
    display(arr,n);
    return 0 ;

}
